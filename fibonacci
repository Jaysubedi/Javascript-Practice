// Fibonacci number
// Numbers gets to previous number, and have gaps of previous number.
//For Example (0+1 = 1, 1+2 = 3, 3+5 =8, 8+13 = 21)
// After 3 there is gap of 2 and after 5, there is a gap of 3.


function fibonacci(n){
  if(x<=1){
  return n;
  }
 return fibonacci(n-1) + fibonacci(n-2)
}

console.log(fibonacci(10));
